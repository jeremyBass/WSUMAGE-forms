<?xml version="1.0"?>
<config>
	<modules>
		<Wsu_WebForms>
			<version>0.1.0</version>
		</Wsu_WebForms>
	</modules>
	<frontend>
		<routers>
			<webforms>
				<use>standard</use>
				<args>
					<module>Wsu_WebForms</module>
					<frontName>webforms</frontName>
				</args>
			</webforms>
			<contacts>
				<args>
					<modules>
						<Wsu_WebForms before="Mage_Contacts">Wsu_WebForms_Contacts</Wsu_WebForms>
					</modules>
				</args>
			</contacts>
			<layout>
				<updates>
					<webforms>
						<file>webforms.xml</file>
					</webforms>
				</updates>
			</layout>
			<translate>
				<modules>
					<Wsu_WebForms>
						<files>
							<default>Wsu_WebForms.csv</default>
						</files>
					</Wsu_WebForms>
				</modules>
			</translate>
		</routers>
	</frontend>
	<admin>
		<routers>
			<webforms>
				<use>admin</use>
				<args>
					<module>Wsu_WebForms</module>
					<frontName>webforms</frontName>
				</args>
			</webforms>
		</routers>
	</admin>
	<adminhtml>
		<translate>
			<modules>
				<Wsu_WebForms>
					<files>
						<default>Wsu_WebForms.csv</default>
					</files>
				</Wsu_WebForms>
			</modules>
		</translate>
		<menu>
			<cms translate="title" module="cms">
				<children>
					<webforms module="webforms">
						<title>Forms</title>
						<sort_order>77</sort_order>
					</webforms>
				</children>
			</cms>
		</menu>
	</adminhtml>
</config>