<?xml version="1.0"?>
<config>
	<modules>
		<Wsu_WebForms>
			<version>0.1.0</version>
		</Wsu_WebForms>
	</modules>
	<frontend>
		<routers>
			<webforms>
				<use>standard</use>
				<args>
					<module>Wsu_WebForms</module>
					<frontName>webforms</frontName>
				</args>
			</webforms>
			<contacts>
				<args>
					<modules>
						<Wsu_WebForms before="Mage_Contacts">Wsu_WebForms_Contacts</Wsu_WebForms>
					</modules>
				</args>
			</contacts>
		</routers>
	</frontend>
</config>